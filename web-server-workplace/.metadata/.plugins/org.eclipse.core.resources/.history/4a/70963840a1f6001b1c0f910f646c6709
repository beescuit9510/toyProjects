package lesson1;

import java.util.ArrayList;
import java.util.List;

public class Run {

	public static void main(String[] args) {
		
		String[] things = {"eggs","lasers","hats","pie"};
		
		List<String> list = new ArrayList<String>();
		list.add(things[0]);
		
		for(String str : things) {
			System.out.println(str);
		}

	}

}
