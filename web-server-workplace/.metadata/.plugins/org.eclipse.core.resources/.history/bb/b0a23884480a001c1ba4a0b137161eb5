package MySort;

public class QuickSort {
	
	public static void main(String[] args) {

//		int[] list = { 1, 2, 3, 4, 9, 8, 7, 2, 1, 3, 10, 12, 16 };
		int[] list = { 4, 2, 1, 16, 3 };
		quickSort(list);

		for (int i : list) {
			System.out.print(i + "\t");
		}
	}

	private static void quickSort(int[] list) {
		int len = list.length;
		int pivot = 0;
		
		for(int i : list)
			pivot+=i;
		
		pivot = pivot/len;
		
		
	}

}
